{
	"name": "remix-template-netlify",
	"private": true,
	"description": "",
	"license": "",
	"sideEffects": false,
	"scripts": {
		"build:remix": "remix build",
		"css:base": "postcss \"app/styles/**/*.scss\" --base app/styles --dir app/dist/styles --ext css",
		"css:routes": "postcss \"app/routes/**/*.scss\" --base app/routes --dir app/dist/styles/routes --ext css",
		"dev:css": "concurrently \"npm:css:* -- --verbose --watch\"",
		"build:css": "cross-env NODE_ENV=production concurrently \"npm:css:*\"",
		"build": "npm run build:css && npm run build:remix",
		"predev": "cross-env NODE_ENV=development npm run build",
		"dev": "concurrently --names \"Remix,CSS\" \"remix dev\" \"npm:dev:css\"",
		"start": "cross-env NODE_ENV=production netlify dev"
	},
	"dependencies": {
		"@cld-apis/types": "^0.1.6",
		"@csstools/postcss-sass": "^5.0.1",
		"@csstools/sass-import-resolve": "^1.0.0",
		"@netlify/functions": "^1.3.0",
		"@remix-run/dev": "1.7.4",
		"@remix-run/netlify": "1.7.4",
		"@remix-run/react": "1.7.4",
		"autoprefixer": "^10.4.13",
		"cloudinary-build-url": "^0.2.4",
		"clsx": "^1.2.1",
		"concurrently": "^7.5.0",
		"cross-env": "^7.0.3",
		"cssnano": "^5.1.14",
		"escape-goat": "^4.0.0",
		"front-matter": "^4.0.2",
		"lodash": "^4.17.21",
		"lru-cache": "^7.14.1",
		"mdx-bundler": "^9.0.1",
		"mitt": "^3.0.0",
		"node-sass-alias-importer": "^2.0.0",
		"parse-numeric-range": "^1.3.0",
		"postcss": "^8.4.19",
		"postcss-cli": "^10.0.0",
		"postcss-import": "^15.0.0",
		"postcss-preset-env": "^7.8.3",
		"postcss-scss": "^4.0.5",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"rehype-slug": "^5.1.0",
		"rehype-stringify": "^9.0.3",
		"remark": "^14.0.2",
		"remark-gfm": "^3.0.1",
		"remark-parse": "^10.0.1",
		"remark-rehype": "^10.1.0",
		"remix": "1.7.4",
		"sass": "^1.56.1",
		"shiki": "0.10.1",
		"tiny-invariant": "^1.2.0",
		"unified": "^10.1.2",
		"unist-util-visit": "^4.1.0"
	},
	"devDependencies": {
		"@remix-run/eslint-config": "1.7.4",
		"@remix-run/serve": "1.7.4",
		"@types/hast": "^2.3.4",
		"@types/lodash": "^4.14.188",
		"@types/lru-cache": "^7.10.10",
		"@types/react": "^17.0.24",
		"@types/react-dom": "^17.0.9",
		"@types/unist": "^2.0.6",
		"eslint": "^8.27.0",
		"prettier": "^2.7.1",
		"typescript": "^4.8.4"
	},
	"engines": {
		"node": ">=14"
	}
}
