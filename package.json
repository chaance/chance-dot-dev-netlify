{
	"name": "remix-template-netlify",
	"private": true,
	"description": "",
	"license": "",
	"sideEffects": false,
	"scripts": {
		"build:remix": "remix build",
		"generate:css": "postcss \"app/styles/**/*.css\" --base app/styles --dir app/dist/styles",
		"build:css": "npm run generate:css -- --env production",
		"build": "npm run build:css && npm run build:remix",
		"predev": "cross-env NODE_ENV=development npm run build",
		"dev": "concurrently --names \"Netlify,CSS\" \"netlify dev\" \"npm run dev:css\"",
		"dev:css": "npm run generate:css -- --watch --verbose",
		"postinstall": "remix setup node",
		"start": "cross-env NODE_ENV=production netlify dev"
	},
	"dependencies": {
		"@netlify/functions": "^0.10.0",
		"@remark-embedder/core": "^2.0.0",
		"@remark-embedder/transformer-oembed": "^2.0.0",
		"@remix-run/dev": "^1.3.4",
		"@remix-run/netlify": "^1.3.4",
		"@remix-run/react": "^1.3.4",
		"@tailwindcss/aspect-ratio": "0.4.0",
		"@tailwindcss/typography": "0.5.2",
		"clsx": "^1.1.1",
		"cssnano": "^5.1.7",
		"escape-goat": "^4.0.0",
		"front-matter": "^4.0.2",
		"lodash": "^4.17.21",
		"lru-cache": "^7.7.3",
		"mdx-bundler": "^9.0.0",
		"mitt": "^3.0.0",
		"p-queue": "^7.2.0",
		"parse-numeric-range": "^1.3.0",
		"postcss": "^8.4.12",
		"postcss-100vh-fix": "^1.0.2",
		"postcss-cli": "^9.1.0",
		"postcss-import": "^14.1.0",
		"postcss-load-config": "^3.1.4",
		"postcss-preset-env": "^7.4.3",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"reading-time": "^1.5.0",
		"rehype-document": "^6.0.1",
		"rehype-format": "^4.0.1",
		"rehype-parse": "^8.0.4",
		"rehype-stringify": "^9.0.3",
		"remark": "^14.0.2",
		"remark-frontmatter": "^4.0.1",
		"remark-gfm": "^3.0.1",
		"remark-html": "^15.0.1",
		"remark-slug": "^7.0.1",
		"remix": "^1.3.4",
		"shiki": "0.10.1",
		"tiny-invariant": "^1.2.0",
		"unified": "^10.1.2",
		"unist-util-visit": "^4.1.0"
	},
	"devDependencies": {
		"@remix-run/eslint-config": "^1.3.4",
		"@types/hast": "^2.3.4",
		"@types/lodash": "^4.14.181",
		"@types/lru-cache": "^7.6.1",
		"@types/mdast": "^3.0.10",
		"@types/react": "^17.0.24",
		"@types/react-dom": "^17.0.9",
		"@types/unist": "^2.0.6",
		"concurrently": "^7.1.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.11.0",
		"prettier": "^2.6.1",
		"typescript": "^4.5.5"
	},
	"engines": {
		"node": ">=14"
	}
